<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.swiftmind">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_intro_slide1"
        android:label="Swift Mind"
        android:roundIcon="@drawable/ic_intro_slide1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.swiftmind.NotepadEntry"></activity>
        <activity
            android:name="com.example.swiftmind.MainActivity"
            android:clearTaskOnLaunch="true"
            android:finishOnTaskLaunch="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.swiftmind.intro.IntroActivity" />

        <receiver android:name="com.example.swiftmind.notification.AlarmReceiver" />
    </application>

</manifest>